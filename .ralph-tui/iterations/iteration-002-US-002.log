994â†’    }\n   995â†’\n   996â†’    /**\n   997â†’     * Load answers from localStorage or provided data\n   998â†’     */\n   999â†’    loadFormAnswers(externalData = null) {\n  1000â†’        const data = externalData || JSON.parse(localStorage.getItem('investigation_form_data') || '{}');\n  1001â†’        Object.entries(data).forEach(([id, value]) => {\n  1002â†’            const el = document.getElementById(id);\n  1003â†’            if (el) {\n  1004â†’                if (el.type !== 'file') {\n  1005â†’                    el.value = value;\n  1006â†’                } else {\n  1007â†’                    // It's a file input. Check if we have a recorded blob for it\n  1008â†’                    // The filenames and checkmarks are handled by AutoSaveManager.updateFileUI\n  1009â†’                    // during the cloud sync, but for local-only load we might need a fallback.\n  1010â†’                }\n  1011â†’            }\n  1012â†’        });\n  1013â†’    }\n  1014â†’\n  1015â†’    /**\n  1016â†’     * Save single answer to localStorage\n  1017â†’     */\n  1018â†’    saveFormAnswer(id, value) {\n  1019â†’        const data = JSON.parse(localStorage.getItem('investigation_form_data') || '{}');\n  1020â†’        data[id] = value;\n  1021â†’        localStorage.setItem('investigation_form_data', JSON.stringify(data));\n  1022â†’    }\n  1023â†’\n  1024â†’\n  1025â†’\n  1026â†’    /**\n  1027â†’     * Submit data to Netlify Forms\n  1028â†’     */\n  1029â†’    async submitToNetlify() {\n  1030â†’        const btn = this.contentEl.querySelector('#btn-submit-netlify');\n  1031â†’        const originalText = btn.innerHTML;\n  1032â†’        btn.disabled = true;\n  1033â†’        btn.innerHTML = 'Enviando...';\n  1034â†’        this.updateSaveStatus('Enviando...');\n  1035â†’\n  1036â†’        try {\n  1037â†’            const data = JSON.parse(localStorage.getItem('investigation_form_data') || '{}');\n  1038â†’            const contextInfo = `Data: ${new Date().toLocaleString()} | UserAgent: ${navigator.userAgent}`;\n  1039â†’\n  1040â†’            // Prepare FormData for Netlify\n  1041â†’            const formData = new FormData();\n  1042â†’            formData.append('form-name', 'investigation-data');\n  1043â†’            formData.append('context_info', contextInfo);\n  1044â†’\n  1045â†’            // Handle Attachments\n  1046â†’            const fileInputs = this.contentEl.querySelectorAll('.form-attachment');\n  1047â†’            const attachmentMap = {};\n  1048â†’            let fileCount = 0;\n  1049â†’\n  1050â†’            fileInputs.forEach((input) => {\n  1051â†’                if (input.files.length > 0 && fileCount < 10) {\n  1052â†’                    const file = input.files[0];\n  1053â†’                    const fieldName = `attachment_${fileCount}`;\n  1054â†’\n  1055â†’                    // Append file to FormData\n  1056â†’                    formData.append(fieldName, file);\n  1057â†’\n  1058â†’                    // Record metadata\n  1059â†’                    attachmentMap[fieldName] = {\n  1060â†’                        originalName: file.name,\n  1061â†’                        fieldId: input.id,\n  1062â†’                        context: input.dataset.context || 'Unknown'\n  1063â†’                    };\n  1064â†’\n  1065â†’                    fileCount++;\n  1066â†’                }\n  1067â†’            });\n  1068â†’\n  1069â†’            // Add attachments map to payload\n  1070â†’            data._attachments_map = attachmentMap;\n  1071â†’\n  1072â†’            // Add main payload\n  1073â†’            formData.append('payload', JSON.stringify(data, null, 2));\n  1074â†’\n  1075â†’            // Send via Fetch (Content-Type is set automatically for FormData)\n  1076â†’            const response = await fetch('/', {\n  1077â†’                method: 'POST',\n  1078â†’                body: formData\n  1079â†’            });\n  1080â†’\n  1081â†’            if (response.ok) {\n  1082â†’                alert('âœ… Dados e arquivos enviados com sucesso para a nuvem!');\n  1083â†’                this.updateSaveStatus('Enviado para Nuvem');\n  1084â†’            } else {\n  1085â†’                throw new Error('Erro na resposta do servidor');\n  1086â†’            }\n  1087â†’\n  1088â†’        } catch (error) {\n  1089â†’            console.error('Netlify Submit Error:', error);\n  1090â†’            alert('âŒ Erro ao enviar dados. Verifique sua conexÃ£o.');\n  1091â†’            this.updateSaveStatus('Erro no envio');\n  1092â†’        } finally {\n  1093â†’            btn.innerHTML = originalText;\n  1094â†’            btn.disabled = false;\n  1095â†’        }\n  1096â†’    }\n  1097â†’\n  1098â†’    /**\n  1099â†’     * Render War Room Dashboard (Article 08)\n  1100â†’     * Transforms intervention plan into a Kanban-style board\n  1101â†’     */\n  1102â†’    renderWarRoom(html) {\n  1103â†’        const div = document.createElement('div');\n  1104â†’        div.innerHTML = html;\n  1105â†’\n  1106â†’        // Extract title from H1\n  1107â†’        const h1 = div.querySelector('h1');\n  1108â†’        const title = h1 ? h1.textContent : 'Plano de IntervenÃ§Ã£o';\n  1109â†’        if (h1) h1.remove();\n  1110â†’\n  1111â†’\n  1112â†’        // Phase configuration\n  1113â†’        const phases = [\n  1114â†’            { num: 1, name: 'TORNIQUETE', subtitle: 'Imediato', class: 'phase-1' },\n  1115â†’            { num: 2, name: 'DESCOMPRESSÃƒO', subtitle: 'Curto Prazo', class: 'phase-2' },\n  1116â†’            { num: 3, name: 'REESTRUTURAÃ‡ÃƒO', subtitle: 'MÃ©dio Prazo', class: 'phase-3' },\n  1117â†’            { num: 4, name: 'REPOSICIONAMENTO', subtitle: 'Longo Prazo', class: 'phase-4' }\n  1118â†’        ];\n  1119â†’\n  1120â†’        // Parse interventions by phase\n  1121â†’        const phaseData = phases.map(p => ({ ...p, interventions: [] }));\n  1122â†’        let currentPhase = null;\n  1123â†’\n  1124â†’        // Find all H2s (