ed Mode: Ignore URL ID. We always want the master record.\n    42â†’            // Also ignore local storage ID for the purpose of initial connection (we want latest from server)\n    43â†’            // But we keep local storage loaded just in case we are offline (constructor handles this)\n    44â†’\n    45â†’            // If we are online, fetchLatestForUnit will overwrite this.submissionId\n    46â†’            // Restore definition of meta\n    47â†’            const meta = JSON.parse(localStorage.getItem(this.metaKey) || '{}');\n    48â†’            this.submissionId = meta.submissionId || null;\n    49â†’\n    50â†’            // Clean URL if it has an ID, to avoid confusion\n    51â†’            const currentUrl = new URL(window.location);\n    52â†’            if (currentUrl.searchParams.has('id')) {\n    53â†’                currentUrl.searchParams.delete('id');\n    54â†’                window.history.replaceState({}, '', currentUrl);\n    55â†’            }\n    56â†’\n    57â†’            // FORCE MASTER SLUG\n    58â†’            // FORCE GENERAL SLUG\n    59â†’            this.unitSlug = 'general';\n    60â†’        } catch (e) {\n    61â†’            console.warn('Failed to load form meta:', e);\n    62â†’        }\n    63â†’    }\n    64â†’\n    65â†’    /**\n    66â†’     * Save metadata to localStorage\n    67â†’     */\n    68â†’    saveMeta() {\n    69â†’        localStorage.setItem(this.metaKey, JSON.stringify({\n    70â†’            submissionId: this.submissionId,\n    71â†’            unitSlug: this.unitSlug,\n    72â†’            lastSync: new Date().toISOString()\n    73â†’        }));\n    74â†’    }\n    75â†’\n    76â†’    /**\n    77â†’     * Get all answers from localStorage\n    78â†’     */\n    79â†’    getAnswers() {\n    80â†’        try {\n    81â†’            const val = JSON.parse(localStorage.getItem(this.storageKey) || '{}');\n    82â†’            // Safeguard against double-stringification\n    83â†’            return typeof val === 'string' ? JSON.parse(val) : val;\n    84â†’        } catch (e) {\n    85â†’            return {};\n    86â†’        }\n    87â†’    }\n    88â†’\n    89â†’    /**\n    90â†’     * Save single answer to localStorage and trigger debounced sync\n    91â†’     */\n    92â†’    saveAnswer(fieldId, value) {\n    93â†’        const answers = this.getAnswers();\n    94â†’        answers[fieldId] = value;\n    95â†’        localStorage.setItem(this.storageKey, JSON.stringify(answers));\n    96â†’\n    97â†’        this.debouncedSync();\n    98â†’    }\n    99â†’\n   100â†’    /**\n   101â†’     * Clear local data and reset submission\n   102â†’     */\n   103â†’    clearData() {\n   104â†’        localStorage.removeItem(this.storageKey);\n   105â†’        localStorage.removeItem(this.metaKey);\n   106â†’        this.submissionId = null;\n   107â†’    }\n   108â†’\n   109â†’    /**\n   110â†’     * Update status indicator UI\n   111â†’     */\n   112â†’    updateStatus(status, type = 'info') {\n   113â†’        const statusEl = document.getElementById('form-save-status');\n   114â†’        if (!statusEl) return;\n   115â†’\n   116â†’        const icons = {\n   117â†’            saving: 'â˜ï¸',\n   118â†’            saved: 'âœ…',\n   119â†’            offline: 'ğŸ“´',\n   120â†’            error: 'âš ï¸'\n   121â†’        };\n   122â†’\n   123â†’        statusEl.textContent = `${icons[type] || ''} ${status}`;\n   124â†’        statusEl.className = `toolbar-status status-${type}`;\n   125â†’    }\n   126â†’\n   127â†’    /**\n   128â†’     * Debounced sync - waits for user to stop typing\n   129â†’     */\n   130â†’    debouncedSync() {\n   131â†’        this.updateStatus('Salvando...', 'saving');\n   132â†’\n   133â†’        clearTimeout(this.syncTimer);\n   134â†’        this.syncTimer = setTimeout(() => {\n   135â†’            this.syncToCloud();\n   136â†’        }, this.debounceMs);\n   137â†’    }\n   138â†’\n   139â†’    /**\n   140â†’     * Main sync function - sends data to Neon via Netlify Function\n   141â†’     */\n   142â†’    async syncToCloud() {\n   143â†’        if (this.isSyncing) return;\n   144â†’        if (!navigator.onLine) {\n   145â†’            this.updateStatus('Offline - salvo local', 'offline');\n   146â†’            return;\n   147â†’        }\n   148â†’\n   149â†’        this.isSyncing = true;\n   150â†’        this.updateStatus('Sincronizando...', 'saving');\n   151â†’\n   152â†’        try {\n   153â†’            const answers = this.getAnswers();\n   154â†’            const answersMetadata = this.collectFieldMetadata();\n   155â†’\n   156â†’            const payload = {\n   157â†’                submission_id: this.submissionId,\n   158â†’                unit_slug: this.unitSlug,\n   159â†’                answers: answers,\n   160â†’                answers_metadata: answersMetadata,\n   161â†’                respondent_info: {\n   162â†’                    userAgent: navigator.userAgent,\n   163â†’                    timestamp: new Date().toISOString()\n   164â†’                }\n   165â†’            };\n   166â†’\n   167â†’            const response = await fetch(`${this.apiBase}/api/sync-submissions`, {\n   168â†’                method: 'POST',\n   169â†’                headers: { 'Content-Type': 'application/json' },\n   170â†’                body: JSON.stringify(payload)\n   171â†’            });\n   172â†’\n   173â†’            if (!response.ok) {\n   174â†’                let errorMsg = `HTTP ${response.status}`;\n   175â†’                try {\n   176â†’                    const errResult = await response.json();\n   177â†’                    if (errResult.error) errorMsg += `: ${errResult.error}`;\n   178â†’                } catch (e) { /* ignore parse error */ }\n   179â†’                throw new Error(errorMsg);\n   180â†’            }\n   181â†’\n   182â†’            const result = await response.json();\n   183â†’\n   184â†’            if (result.success) {\n   185â†’            