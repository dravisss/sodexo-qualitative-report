;\n  padding: var(--spacing-lg) var(--page-padding);\n  background-color: var(--color-bg);\n}\n\n.kanban-board {\n  display: flex;\n  gap: var(--spacing-md);\n  height: 100%;\n  min-width: fit-content;\n}\n\n/* === COLUMNS === */\n.kanban-column {\n  width: 300px;\n  background: var(--color-bg-card-alt);\n  border-radius: var(--radius-md);\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n  border: 1px solid var(--color-border);\n  flex-shrink: 0;\n}\n\n.kanban-column-header {\n  padding: var(--spacing-md);\n  border-bottom: 1px solid var(--color-border-light);\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background: rgba(255, 255, 255, 0.5);\n  border-radius: var(--radius-md) var(--radius-md) 0 0;\n}\n\n.column-title {\n  font-weight: var(--font-weight-bold);\n  font-size: var(--font-size-sm);\n  color: var(--color-text-secondary);\n  display: flex;\n  align-items: center;\n  gap: var(--spacing-sm);\n}\n\n.column-count {\n  background: var(--color-border-light);\n  padding: 2px 8px;\n  border-radius: var(--radius-full);\n  font-size: var(--font-size-xs);\n  color: var(--color-text-muted);\n}\n\n.kanban-column-body {\n  padding: var(--spacing-sm);\n  overflow-y: auto;\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  gap: var(--spacing-sm);\n}\n\n/* === STATUS COLORS === */\n.status-backlog .column-title { color: var(--color-text-muted); }\n.status-todo .column-title { color: var(--color-info); }\n.status-doing .column-title { color: var(--color-warning); }\n.status-blocked .column-title { color: var(--color-error); }\n.status-done .column-title { color: var(--color-success); }\n\n.status-backlog { border-top: 3px solid var(--color-text-light); }\n.status-todo { border-top: 3px solid var(--color-info); }\n.status-doing { border-top: 3px solid var(--color-warning); }\n.status-blocked { border-top: 3px solid var(--color-error); }\n.status-done { border-top: 3px solid var(--color-success); }\n\n\n/* === CARDS === */\n.kanban-card {\n  background: var(--color-bg-card);\n  padding: var(--spacing-md);\n  border-radius: var(--radius-sm);\n  box-shadow: var(--shadow-sm);\n  border: 1px solid var(--color-border);\n  cursor: grab;\n  transition: var(--transition-fast);\n}\n\n.kanban-card:hover {\n  box-shadow: var(--shadow-md);\n  transform: translateY(-2px);\n  border-color: var(--color-teal);\n}\n\n.kanban-card.dragging {\n  opacity: 0.5;\n  cursor: grabbing;\n  border: 1px dashed var(--color-teal);\n}\n\n/* SortableJS Classes */\n.sortable-ghost {\n  opacity: 0.4;\n  background-color: var(--color-bg-subtle);\n  border: 1px dashed var(--color-teal);\n}\n\n.sortable-drag {\n  cursor: grabbing;\n  opacity: 1;\n  background: var(--color-bg-card);\n  box-shadow: var(--shadow-lg);\n  transform: scale(1.02);\n}\n\n.kanban-column-body.drag-over {\n  background: rgba(0, 150, 136, 0.1); /* Teal tint */\n  border: 2px dashed var(--color-teal);\n  border-radius: var(--radius-md);\n}\n\n.card-id {\n  font-size: var(--font-size-xs);\n  color: var(--color-text-light);\n  font-weight: var(--font-weight-medium);\n  margin-bottom: var(--spacing-xs);\n}\n\n.card-title {\n  font-size: var(--font-size-sm);\n  color: var(--color-text);\n  line-height: var(--line-height-normal);\n  font-weight: var(--font-weight-medium);\n}\n\n.card-tags {\n  margin-top: var(--spacing-sm);\n  display: flex;\n  flex-wrap: wrap;\n  gap: 4px;\n}\n\n.card-tag {\n  font-size: 10px;\n  padding: 2px 6px;\n  border-radius: 4px;\n  background: var(--color-bg);\n  color: var(--color-text-muted);\n  border: 1px solid var(--color-border);\n}\n\n/* === LOADING === */\n.loading {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 100%;\n    width: 100%;\n    color: var(--color-text-muted);\n    font-style: italic;\n}\n\n/* === NAV EXTENSIONS === */\n.nav-back-link {\n    padding: 0 var(--spacing-md) var(--spacing-sm);\n}\n\n.nav-back-link .nav-link {\n    background: var(--color-bg);\n    border: 1px solid var(--color-border);\n    justify-content: center;\n}\n\n.nav-divider {\n    height: 1px;\n    background: var(--color-border);\n    margin: var(--spacing-sm) var(--spacing-md);\n}\n\n/* === RESPONSIVE === */\n@media (max-width: 768px) {\n    .kanban-board-container {\n        padding: var(--spacing-md);\n    }\n\n    .kanban-column {\n        width: 260px;\n    }\n}\n","numLines":221,"startLine":1,"totalLines":221}}}
